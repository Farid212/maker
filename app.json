{
  "expo": {
    "name": "Maker - ToDo list",
    "slug": "maker",
    "description": "Maker is a advanced ToDo mobile application created with React Native. This app working with Android and iOS.",
    "githubUrl": "https://github.com/mateuszpijanowski/maker",
    "privacy": "public",
    "version": "0.5.0",
    "sdkVersion": "32.0.0",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.webStrong.maker"
    },
    "android": {
      "package": "com.webStrong.maker"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "primaryColor": "#f4511e",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}